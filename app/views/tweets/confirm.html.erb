<h1>投稿確認画面</h1>

<%= form_with(model: @tweet, local: true, url: tweets_path) do |f| %>
  <%= f.hidden_field :content %>
  
  <p>投稿内容</p>
  <p><%= f.content %></p>
  <p><%= f.submit "投稿する" %></p>
  
  <%= form_with(model: @tweet, local: true, url: new_tweet_path, method: :get) do |f| %>
     <%= f.hidden_field :content %>
     <p><%= f.submit "戻る", name: "back" %></p>
  <% end %>

<% end %>